{
  "name": "sebspark",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "lint:format": "prettier --cache --check --ignore-path .gitignore \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "release": "npm run build && npx changeset publish",
    "test": "FORCE_COLOR=1 turbo test",
    "changeset": "changeset",
    "generate:package": "turbo gen package",
    "typecheck": "FORCE_COLOR=1 turbo typecheck",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "private": true,
  "dependencies": {
    "@changesets/cli": "2.26.2"
  },
  "devDependencies": {
    "@turbo/gen": "1.10.6",
    "@types/node": "18.16.18",
    "prettier": "2.8.8",
    "tsup": "7.1.0",
    "turbo": "1.10.6",
    "typescript": "5.1.6",
    "vitest": "0.32.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@1.22.19"
}
